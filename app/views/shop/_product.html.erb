<% @shops.reverse.each do |post| %>
  <div class="panel">
    <div class="row panel-body">
      <div class="col-md-3">
      </div>
      <div class="col-md-3">
        <img src="<%= post.image_url(:thumb) %>">
      </div>
      <div class="col-md-6">
        <h4><a href="#"><%= post.title %></a></h4>
      </div>
    </div>
  </div>
<% end %>
